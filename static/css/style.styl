.select {
  color gris_oscuro
  display: block;
  position: relative;
  overflow: hidden;
  background: #F8F8F8;
  border-bottom-color: #c5d4d9;
  border-bottom 1px solid #bbb
  font-size 15px
  margin 0px 0 6px 32px
  outline: 0px none;
  padding 0
  width 84%
  height 33px
  line-height 3
}
.select:before, .select:after {
  content: '';
  position: absolute;
  right: 11px;
  width: 0;
  height: 0;
  border-left: 3px outset transparent;
  border-right: 3px outset transparent;
}
.select:before {
  top: 18px;
  border-bottom: 3px solid #7f9298;
}
.select:after {
  top: 24px;
  border-top: 3px solid #7f9298;
}
.select > select {
  position: relative;
  z-index: 2;
  width: 110%;
  height: 29px;
  line-height: 17px;
  padding: 5px 1px;
  color: gris_oscuro;
  background: transparent;
  background: rgba(0, 0, 0, 0);
  border: 0;
  -webkit-appearance: none;
}
.select > select:focus {
  color: #4d5a5e;
  outline: 0;
}



