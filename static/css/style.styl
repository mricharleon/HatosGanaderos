.cinta {
		position: relative;
		color: #fff;
		display: inline-block;
		padding: 0 4px 4px 0;
		overflow: hidden;
		font-family: Arial, sans-serif;
		font-weight: 400;
		margin 0
	} 
	
	.cinta p {
		display: inline;
		font-size 13px	
	}
	
	.cinta p span {
		z-index 3
		position: absolute;
		display: inline-block;
		left: -20px;
		box-shadow: 0px 0px 10px rgba(0,0,0,0.2), inset 0px 5px 30px rgba(0,0,0,0.2);
		text-align: center;
		text-transform: uppercase;
		top: 7px;
		background: #555;
		width: 68px;
		padding: 1px 0px 1px 0px;
		-webkit-transform: rotate(-45deg);
		-moz-transform: rotate(-45deg);
		-o-transform: rotate(-45deg);
		-ms-transform: rotate(-45deg);
	} 
	
	.cinta p:before {
		content: "";
		width: 0;
		height: 0;
		position: absolute;
		top: -17px;
		left: 24px;
		z-index: -1;
		border: 17px solid;
		border-color: transparent transparent #333 transparent;
	}
	
	.cinta p:after {
		content: "";
		width: 0px;
		height: 0px;
		position: absolute;
		top: 37px;
		z-index: -10;
		left: -5px;
		border: 10px solid;
		border-color: #222 transparent transparent transparent;
	}



