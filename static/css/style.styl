// css radial menu

#profile-nav
	list-style none
	margin 0
	padding 0

#profile-nav ul {
  list-style: none;
  margin: 0px;
  padding: 0px;
}

#profile-nav li{
	float: none; 
  	width: 100%;
  	transition: all 8s ease 0s;
  	word-wrap break-word
  	font-family 'Raleway'
}


#profile-nav li a{
  display: block; 
  width: 100%; 
  padding: 13px; 
  border-right: 5px solid; 
  position: relative; 
  z-index: 2;
  text-decoration: none;
  color: #444;
  box-sizing: border-box;  
  -moz-box-sizing: border-box;  
  -webkit-box-sizing: border-box; 
}
	
#profile-nav li a:hover{ border-bottom: 0px; color: #fff;}
#profile-nav li:first-child a{ border-right: 5px solid #3698C9; }
#profile-nav li:nth-child(2) a{ border-right: 5px solid #00AE87; }
#profile-nav li:nth-child(3) a{ border-right: 5px solid #FFC663; }
#profile-nav li:nth-child(4) a{ border-right: 5px solid #DD6077; }
#profile-nav li:last-child a{ border-right: 5px solid #DF3345; }

#profile-nav li a:before { 
  content: "";
  height: 100%; 
  right: 0; 
  top: 0; 
  width: 0px;  
  position: absolute; 
  transition: all 0.3s ease 0s; 
  -webkit-transition: all 0.3s ease 0s; 
  -o-transition-duration: 0.3s; 
  z-index: -1;
}

#profile-nav li a:hover:before{ width: 100%; }
#profile-nav li:first-child a:before{ background: #3698C9; }
#profile-nav li:nth-child(2) a:before{ background: #00AE87; }
#profile-nav li:nth-child(3) a:before{ background: #FFC663; }
#profile-nav li:nth-child(4) a:before{ background: #DD6077; }
#profile-nav li:last-child a:before{ background: #DF3345; }

